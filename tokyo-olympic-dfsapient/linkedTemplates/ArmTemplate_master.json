{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "tokyo-olympic-dfsapient"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AthletesHttp_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://raw.githubusercontent.com/darshilparmar/tokyo-olympic-azure-data-engineering-project/main/data/Athletes.csv"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tokyoolympicssapientai.dfs.core.windows.net/"
		},
		"CoachesHttp_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://raw.githubusercontent.com/darshilparmar/tokyo-olympic-azure-data-engineering-project/main/data/Coaches.csv"
		},
		"EntriesGenderHttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://raw.githubusercontent.com/darshilparmar/tokyo-olympic-azure-data-engineering-project/main/data/EntriesGender.csv"
		},
		"MedalsHttp_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://raw.githubusercontent.com/darshilparmar/tokyo-olympic-azure-data-engineering-project/main/data/Medals.csv"
		},
		"TeamsHttp_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://raw.githubusercontent.com/darshilparmar/tokyo-olympic-azure-data-engineering-project/main/data/Teams.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "tokyo-olympic-dfsapient_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AthletesHttp_properties_typeProperties_url": {
						"value": "[parameters('AthletesHttp_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"CoachesHttp_properties_typeProperties_url": {
						"value": "[parameters('CoachesHttp_properties_typeProperties_url')]"
					},
					"EntriesGenderHttpServer1_properties_typeProperties_url": {
						"value": "[parameters('EntriesGenderHttpServer1_properties_typeProperties_url')]"
					},
					"MedalsHttp_properties_typeProperties_url": {
						"value": "[parameters('MedalsHttp_properties_typeProperties_url')]"
					},
					"TeamsHttp_properties_typeProperties_url": {
						"value": "[parameters('TeamsHttp_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}